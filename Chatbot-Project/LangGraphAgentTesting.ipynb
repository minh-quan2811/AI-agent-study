    "                for element in elements:\n",
    "                    text = element.get_text(separator=' ', strip=True)\n",
    "                    if len(text) > 100:\n",
    "                        main_content.append(text)\n",
    "                if main_content:\n",
    "                    content_found = True\n",
    "                    break\n",
    "        \n",
    "        # Fallback to paragraphs\n",
    "        if not content_found:\n",
    "            for tag in ['h1', 'h2', 'h3', 'p']:\n",
    "                elements = soup.find_all(tag)\n",
    "                for element in elements:\n",
    "                    text = element.get_text(separator=' ', strip=True)\n",
    "                    if len(text) > 50:\n",
    "                        main_content.append(text)\n",
    "        \n",
    "        # Clean and format\n",
